<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LinkBuilder">
        <attr name="defaultLinkColor" format="color" />
        <attr name="defaultTextColorOfHighlightedLink" format="color" />
    </declare-styleable>

    <attr name="linkBuilderStyle" format="reference" />

    <declare-styleable name="TypedKtView">
        <attr name="animateOnLoad" format="boolean"/>
        <attr name="delay" format="integer"/>
        <attr name="isLooped" format="boolean"/>
        <attr name="isAnimatedByWord" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ToolbarX">
        <attr name="toolbarX_animationDuration" format="integer" />
        <attr name="toolbarX_showElevationAtStart" format="boolean" />
    </declare-styleable>


    <attr format="color" name="dotsColor"/>
    <attr format="color" name="selectedDotColor"/>
    <attr format="color" name="dotsStrokeColor"/>
    <attr format="dimension" name="dotsSize"/>
    <attr format="float" name="dotsWidthFactor"/>
    <attr format="dimension" name="dotsSpacing"/>
    <attr format="dimension" name="dotsCornerRadius"/>
    <attr format="dimension" name="dotsStrokeWidth"/>
    <attr format="float" name="stiffness"/>
    <attr format="float" name="dampingRatio"/>
    <attr format="boolean" name="progressMode"/>

    <declare-styleable name="DotsIndicator">
        <attr name="dotsColor"/>
        <attr name="selectedDotColor"/>
        <attr name="dotsSize"/>
        <attr name="dotsWidthFactor"/>
        <attr name="dotsSpacing"/>
        <attr name="dotsCornerRadius"/>
        <attr name="progressMode"/>
    </declare-styleable>

    <declare-styleable name="SpringDotsIndicator">
        <attr name="dotsColor"/>
        <attr name="dotsSize"/>
        <attr name="dotsSpacing"/>
        <attr name="dotsCornerRadius"/>
        <attr name="dotsStrokeWidth"/>
        <attr name="dotsStrokeColor"/>
        <attr name="stiffness"/>
        <attr name="dampingRatio"/>
    </declare-styleable>

    <declare-styleable name="WormDotsIndicator">
        <attr name="dotsColor"/>
        <attr name="dotsSize"/>
        <attr name="dotsSpacing"/>
        <attr name="dotsCornerRadius"/>
        <attr name="dotsStrokeWidth"/>
        <attr name="dotsStrokeColor"/>
    </declare-styleable>

</resources>